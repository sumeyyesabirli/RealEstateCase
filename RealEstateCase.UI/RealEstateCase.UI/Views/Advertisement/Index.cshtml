@{
    Layout = null;
}
<!DOCTYPE html>
<html dir="ltr" lang="en-US">

<head>
    <!-- Document Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- IE Compatibility Meta -->
    <meta name="author" content="zytheme" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Real Estate html5 template">
    <link href="~/MAIN_FILES/Lalan/assets/images/favicon/favicon.png" rel="icon">

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i,800,800i%7CPoppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Stylesheets -->
    <link href="~/MAIN_FILES/Lalan/assets/css/external.css" rel="stylesheet">
    <link href="~/MAIN_FILES/Lalan/assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/MAIN_FILES/Lalan/assets/css/style.css" rel="stylesheet">

    <!-- HTML5 shim for IE6-8 support -->
    <!--[if lt IE 9]>
      <script src="assets/js/html5shiv.js"></script>
      <script src="assets/js/respond.min.js"></script>
    <![endif]-->
    <!-- Document Title -->
    <title>@ViewData["Title"] - Real Estate Html5 Template</title>
</head>
<body>
    <!-- Document Wrapper -->
    <div id="wrapper" class="wrapper clearfix">
        <!-- Navbar Partial -->
        @Html.Partial("~/Views/Shared/Components/_AdvertisementParital/_NavbarsPartial.cshtml")

        <!-- Map Partial -->
        @Html.Partial("~/Views/Shared/Components/_AdvertisementParital/_MapPartial.cshtml")
        <!-- Properties Grid Partial -->
        @Html.Partial("~/Views/Shared/Components/_AdvertisementParital/_PropertiesGridPartial.cshtml")

        <!-- CTA Section -->
        <section id="cta" class="cta cta-1 text-center bg-overlay bg-overlay-dark pt-90">
            <div class="bg-section">
                <img src="~/MAIN_FILES/Lalan/assets/images/cta/bg-1.jpg" alt="Background">
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-6 col-md-offset-3">
                        <h3>Join our professional team & agents to start selling your house</h3>
                        <a href="#" class="btn btn--primary">Contact</a>
                    </div>
                </div>
            </div>
        </section>

        <!-- Footer Partial -->
        @Html.Partial("~/Views/Shared/Components/_AdvertisementParital/_FooterPartial.cshtml")
    </div>

    <!-- Footer Scripts -->
    <script src="~/MAIN_FILES/Lalan/assets/js/jquery-2.2.4.min.js"></script>
    <script src="~/MAIN_FILES/Lalan/assets/js/plugins.js"></script>
    <script src="~/MAIN_FILES/Lalan/assets/js/functions.js"></script>
    <script src="http://maps.google.com/maps/api/js?sensor=true&amp;key=AIzaSyCiRALrXFl5vovX0hAkccXXBFh7zP8AOW8"></script>
    <script src="~/MAIN_FILES/Lalan/assets/js/plugins/jquery.gmap.min.js"></script>
    <script src="~/MAIN_FILES/Lalan/assets/js/map-addresses.js"></script>
    <script src="~/MAIN_FILES/Lalan/assets/js/map-custom.js"></script>
</body>

</html>
